<div class="panel-heading">
  <h3>Add Service</h3>
</div>
<div class="panel-body">
  <%= form_for @portfolio_service, url: partner_company_services_path, html: { multipart: true, :class => 'form-horizontal', id: 'company_form' } do |form| %>

   <div class="col-sm-12">
      <div class="col-sm-6 col-sm-offset-3">
         <div class="col-sm-4">
         <%= form.label :service, "Sub Service", class: 'control-label col-sm-2' %>
         </div>
         <div class="col-sm-8">
            <div class="form-group">
               <%= select_available_services(@sub_services, form) %>
               <label for="portfolio_service_service_id" class="error error-title"></label>
            </div>
         </div>
      </div>

      <div class="col-sm-6 col-sm-offset-3">
    <div class="form-group" id="show_city"></div>
    </div>

     <div class="col-sm-6 col-sm-offset-3">
         <div class="col-sm-4">
            <%= form.label :price, class: 'control-label col-sm-2' %>
         </div>
         <div class="col-sm-8">
            <div class="form-group">
               <%= form.number_field :price, class: 'form-control form_input' %>
               <label for="portfolio_service_price" class="error error-title"></label>
            </div>
         </div>
      </div>
   </div>

    <div class="actions">
      <%= form.submit "Create", :class=> 'btn btn-primary w3l_header_right', id: "company_service_create" %>
    </div>
  <% end %>
</div>
<script type="text/javascript"> <%= render "common/validate.js" %> </script>
